    <fields>
        <fullName>C501_Tax_Status__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Lookup from Payment to Tax Status - to be populated via Flow</description>
        <externalId>false</externalId>
        <inlineHelpText>To link this Payment to the Payee&apos;s most recent Tax Status, click {button name}</inlineHelpText>
        <label>Tax Status</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>The linked Tax Status must be related to the Payee&apos;s Organization account</errorMessage>
            <filterItems>
                <field>$Source.FGM_Base__Payee__c</field>
                <operation>equals</operation>
                <valueField>FGM_Base__Tax_Status__c.FGM_Base__Organization__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>FGM_Base__Tax_Status__c</referenceTo>
        <relationshipLabel>Payments</relationshipLabel>
        <relationshipName>Payments</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>C501_numTaxStatuses__c</fullName>
        <description>Displays the value of the Payee&apos;s numTaxStatuses field, if there is a Payee on the Payment; used in Flow to decide whether or not to get the most recent Tax Status for the Payee; Not on page layouts</description>
        <externalId>false</externalId>
        <formula>IF( NOT( ISBLANK( FGM_Base__Payee__c )),
    FGM_Base__Payee__r.C501_numTaxStatuses__c,
    null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>numTaxStatuses</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
